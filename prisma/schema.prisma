generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider  = "postgresql"
    url       = env("DATABASE_URL")
    directUrl = env("DIRECT_URL")
}

model Ingredient {
    id        String  @id @default(uuid())
    name      String  @unique
    color     String
    textColor String?
    link      String?
    amount    String
    volunteer String?
}
